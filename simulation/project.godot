; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Camera",
"class": "FreelookCamera",
"language": "GDScript",
"path": "res://scripts/FreelookCamera.gd"
}, {
"base": "MeshInstance",
"class": "Motor",
"language": "GDScript",
"path": "res://class/motor/motor.gd"
}, {
"base": "Reference",
"class": "PolarCoord",
"language": "GDScript",
"path": "res://class/polar_coord.gd"
} ]
_global_script_class_icons={
"FreelookCamera": "",
"Motor": "",
"PolarCoord": ""
}

[application]

config/name="simulation"
run/main_scene="res://scenes/Simulation.tscn"
config/icon="res://icon.png"

[autoload]

Body="*res://lib/body.gd"
Brain="*res://lib/brain.gd"
Lib="*res://lib/lib.gd"
Utils="*res://lib/utils.gd"
Controller="*res://lib/controller.gd"
Debug="*res://debug.gd"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
